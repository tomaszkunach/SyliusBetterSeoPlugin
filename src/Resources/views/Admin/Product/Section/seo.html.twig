{% set form = hookable_metadata.context.form %}
{% set product = hookable_metadata.context.resource %}

<div class="tab-pane {% if hookable_metadata.configuration.active|default(false) %}show active{% endif %}" id="product-seo" role="tabpanel" tabindex="0">
    <div class="card mb-3">
        <div class="card-header">
            <h2 class="card-title">
                {{ 'seo.ui.seo'|trans }}
            </h2>
        </div>
        {% hook 'seo' with { product, form } %}
    </div>
</div>
